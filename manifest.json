{
  "name": "BulkTE",
  "description": "Bulk TinEye using jQuery selector or context menu.",
  "version": "0.1",
  "permissions": [
    "activeTab",
	"contextMenus",
	"http://www.tineye.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "BulkTE",
	"default_popup": "popup.html"
  },
  "manifest_version": 2
}
